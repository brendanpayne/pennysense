{
  "expo": {
    "name": "pennysense",
    "slug": "pennysense",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pennysense",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "package": "com.pennysense.app",
      "splash": {
        "image": "./assets/images/splash-light.png",
        "resizeMode": "contain",
        "backgroundColor": "#f5f5f5",
        "dark": {
          "image": "./assets/images/splash-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#252525"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f5f5f5"
      },
      "package": "com.pennysense.app",
      "splash": {
        "image": "./assets/images/splash-light.png",
        "resizeMode": "contain",
        "backgroundColor": "#f5f5f5",
        "dark": {
          "image": "./assets/images/splash-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#252525"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
